<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:section title="${%X-Developer Client Plugin}">
        <f:entry field="appid" title="${%App ID}">
            <f:textbox/>
        </f:entry>
        <f:entry field="appkey"  title="${%App Key}">
            <f:password/>
        </f:entry>
        <f:validateButton method="testConnection" title="${%Test connection}" with="appid,appkey" />
    </f:section>
</j:jelly>
